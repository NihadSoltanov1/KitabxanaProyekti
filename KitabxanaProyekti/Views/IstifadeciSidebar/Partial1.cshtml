@using KitabxanaProyekti.Models.Entity
@model List<Elan_Table> 


@foreach(var e in Model)
{ 
<div class="post">

    <div class="user-block">

        <img class="img-circle img-bordered-sm"
             src="~/AdminLTE-3.0.4/dist/img/avatar5.png" alt="İstifadəçi Şəkli">

        <span class="username">

            <a href="#">@e.Kategoriya</a>

            <a href="#" class="float-right btn-tool"><i class="fas fa-times"></i></a>

        </span>

        <span class="description">@Convert.ToDateTime(e.Tarix).ToString("dd/MM/yyyy")</span>

    </div>

    <!-- /.user-block -->

    <p>
        @e.Mezmun
    </p>



    <p>
    </p>




</div>
}